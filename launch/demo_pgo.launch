<launch>
<!-- Launch file for Livox AVIA LiDAR -->

  <arg name="rviz" default="true" />
  <rosparam command="load" file="$(find std_detector)/config/config_pgo.yaml" />
  <group if="$(arg rviz)">
    <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find std_detector)/rviz_cfg/pgo.rviz" />
  </group>

  <node 
		pkg="std_detector" 
		type="pgo_demo" 
		name="pgo_demo" 
		output="screen" 
	/>
  <param name="lidar_path" type="string" value = "/media/pcl/定位2/STDesc_dataset/KITTI_dataset/velodyne/"/>
  <param name="pose_path" type="string" value = "/media/pcl/定位2/STDesc_dataset/KITTI_dataset/KITTI_poses_LO/kitti00_pose_lio.txt"/>



</launch>